{
    "manifest_version": 3,
    "name": "WebTrack",
    "version": "1.0",
    "description": "A task tracker Chrome extension that records site URL, for easy access to the task destination.",
    "permissions": ["storage", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "icons": {
      "16": "icons/icon16.png",
       "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
  }